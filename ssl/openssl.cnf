[req]
default_bits = 4096
distinguished_name = req_distinguished_name
req_extensions = v3_req
prompt = no

[req_distinguished_name]
C = US
ST = California
L = San Francisco
O = PixelatedEmpathy
OU = Development
CN = pixelatedempathy.com

[v3_req]
keyUsage = keyEncipherment, dataEncipherment
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = pixelatedempathy.com
DNS.2 = www.pixelatedempathy.com
DNS.3 = *.pixelatedempathy.com
DNS.4 = localhost
DNS.5 = *.localhost
IP.1 = 127.0.0.1
IP.2 = ::1