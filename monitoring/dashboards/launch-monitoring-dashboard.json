{
  "dashboard": {
    "id": null,
    "title": "Launch Monitoring Dashboard",
    "tags": ["launch", "beta", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Launch Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"pixelated-empathy\"}",
            "legendFormat": "Application Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            },
            "mappings": [
              { "options": { "0": { "text": "DOWN" } }, "type": "value" },
              { "options": { "1": { "text": "HEALTHY" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Beta Users",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(pixelated_empathy_active_users)",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 25 }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Session Completion Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum(rate(pixelated_empathy_sessions_completed_total[5m])) / sum(rate(pixelated_empathy_sessions_started_total[5m])) * 100",
            "legendFormat": "Completion Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "green", "value": 80 }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1.5 },
                { "color": "red", "value": 2.0 }
              ]
            },
            "unit": "s",
            "decimals": 2
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 0.5 },
                { "color": "red", "value": 1.0 }
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 4 }
      },
      {
        "id": 6,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "avg_over_time(up{job=\"pixelated-empathy\"}[24h]) * 100",
            "legendFormat": "Uptime %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 99 },
                { "color": "green", "value": 99.9 }
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 4 }
      },
      {
        "id": 7,
        "title": "Crisis Detections",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(pixelated_empathy_crisis_detections_total[24h]))",
            "legendFormat": "Crisis Detections (24h)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 3 }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 }
      },
      {
        "id": 8,
        "title": "Professional Referrals",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(pixelated_empathy_referrals_total[24h]))",
            "legendFormat": "Referrals (24h)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 }
      },
      {
        "id": 9,
        "title": "Request Rate (Requests/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 10,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 11,
        "title": "User Satisfaction Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(pixelated_empathy_user_satisfaction_score)",
            "legendFormat": "Avg Satisfaction"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 5 },
                { "color": "green", "value": 7 }
              ]
            },
            "unit": "none",
            "decimals": 1
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 }
      },
      {
        "id": 12,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 }
      },
      {
        "id": 13,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 }
      },
      {
        "id": 14,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 }
      },
      {
        "id": 15,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "{{alertname}}",
            "format": "table"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Launch Events",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "tags": ["launch"]
        }
      ]
    }
  }
}
