---
export const prerender = false

import { LazySessionsListPage } from '@/components/journal-research/lazy'
import DashboardLayout from '@/layouts/DashboardLayout.astro'

// @ts-ignore Astro.props is injected at runtime
const { user } = Astro.props as { user: any }
---

<DashboardLayout
  title="Research Sessions | Pixelated Empathy"
  description="Manage your journal dataset research sessions"
>
  <div class="container mx-auto py-6 space-y-6">
    <div class="flex items-center justify-between">
      <div>
        <h1 class="text-3xl font-bold">Research Sessions</h1>
        <p class="text-muted-foreground mt-1">
          View and manage all your research sessions
        </p>
      </div>
    </div>

    <LazySessionsListPage client:load />
  </div>
</DashboardLayout>
