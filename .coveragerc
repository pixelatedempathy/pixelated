[run]
source = ai, src
omit =
    */tests/*
    */test_*
    */.venv/*
    */venv/*
    */node_modules/*
    */__pycache__/*
    */migrations/*
    */settings/*
    */manage.py
    */wsgi.py
    */asgi.py
branch = True
parallel = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    \s*if __name__ == \"__main__\":\s*
    class .*\(Protocol\):
    @(abc\.)?abstractmethod
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = Pixelated Empathy AI - Coverage Report

[xml]
output = coverage.xml
