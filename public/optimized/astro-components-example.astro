---
// Example responsive image components
// Copy individual components to your Astro component files as needed
---

---
// Auto-generated responsive image component for android-chrome-192x192
// Usage: <ResponsiveImageAndroid-chrome-192x192 alt="Description" />

export interface Props {
  alt: string;
  loading?: 'lazy' | 'eager';
  class?: string;
}

const { alt, loading = 'lazy', class: className } = Astro.props;
---

<picture class={className}>
  <source
    media="(min-width: 1200px)"
    srcset="/optimizedpublic/android-chrome-192x192-desktop.webp"
    type="image/webp"
  />
  <source
    media="(min-width: 768px)"
    srcset="/optimizedpublic/android-chrome-192x192-tablet.webp"
    type="image/webp"
  />
  <source
    media="(max-width: 767px)"
    srcset="/optimizedpublic/android-chrome-192x192-mobile.webp"
    type="image/webp"
  />
  <img
    src="/optimizedpublic/android-chrome-192x192.webp"
    alt={alt}
    loading={loading}
    decoding="async"
  />
</picture>

